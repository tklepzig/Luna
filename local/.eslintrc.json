{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "no-var": "warn",
        "no-const-assign": "error",
        "no-this-before-super": "warn",
        "no-undef": "error",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "quotes": [
            "warn",
            "double"
        ],
        "no-console": "warn",
        "prefer-const": "warn",
        "prefer-arrow-callback": "warn"
    },
    "extends": [
        "eslint:recommended"
    ]
}